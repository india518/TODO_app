<h1> Here's a List of All our Awesome Users!</h1>
	
<p>
	<% unless @users.empty? %>
		<ul>
		<% @users.each do |user| %>
		  <li><%= link_to user.name, user_url(user.id) %></li>
		<% end %>
	    </ul>
	<% end %>
</p>

<p><%= link_to "Create New User", new_user_url %></p>

<p><%= link_to "Check out all our teams!", teams_url %></p>